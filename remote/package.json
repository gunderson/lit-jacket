{
  "name": "lit-jacket-remote",
  "version": "0.0.1",
  "description": "Lit Jacket",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install": "find ./domains/* -maxdepth 2 -name package.json -execdir npm install \\;",
    "rebuild": "",
    "start-dev": "pm2 start processes.json --env development && pm2 logs",
    "start-prod": "pm2 start processes.json --env production",
    "pull": "git reset --hard && git pull -f",
    "ssh": "gcloud compute --project lit-jacket ssh --zone us-west1-a instance-1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gunderson/lit-jacket.git"
  },
  "author": "patrick@toolofna.com",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/gunderson/lit-jacket/issues"
  },
  "homepage": "https://github.com/gunderson/lit-jacket#readme",
  "dependencies": {
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.0",
    "chalk": "^1.1.3",
    "express": "^4.14.1",
    "express-cache-response-directive": "^1.0.0",
    "fs-extra": "^2.0.0",
    "grunt": "^0.4.5",
    "morgan": "^1.8.0",
    "peak-front-end": "github:gunderson/peak-front-end",
    "socket.io": "^1.7.2"
  },
  "devDependencies": {
    "grunt-fontina": "^0.1.6"
  },
  "engines": {
    "node": "6.9.2^"
  }
}
